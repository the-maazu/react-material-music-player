{
  "name": "react-material-music-player",
  "version": "1.2.2",
  "description": "This is a highly customisable music player powered by mui-v5, typescript, mobile mode, media session action & metadata, reorderable playlist",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.1",
    "@mui/material": "^5.0.1",
    "@reduxjs/toolkit": "^1.6.2",
    "lodash.debounce": "^4.0.8",
    "react-draggable-list": "^4.0.3",
    "react-redux": "^7.2.4"
  },
  "peerDependencies": {
    "react": "^17.0.2 || ^18.0.0",
    "react-dom": "^17.0.2 || ^ 18.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-react-css-modules": "^0.0.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "compile": "babel src/module --delete-dir-on-start --out-dir dist/",
    "typecomp": "tsc src/module/**.js --declaration --allowJs --checkJs --jsx react --esModuleInterop --outDir dist",
    "emit": "yarn typecomp --emitDeclarationOnly",
    "check": "yarn typecomp --noEmit",
    "prestart": "yarn run check --watch > tsc.log &",
    "prebuild": "yarn run check",
    "pretest": "yarn run check",
    "start": "react-scripts --openssl-legacy-provider start",
    "build": "react-scripts --openssl-legacy-provider build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepublishOnly": "yarn compile && yarn emit",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/the-maazu/react-material-music-player.git"
  },
  "keywords": [
    "react",
    "music",
    "player",
    "material",
    "ui",
    "audio",
    "typescript"
  ],
  "author": "maazudev@proton.me",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/the-maazu/react-music-player/issues"
  },
  "homepage": "https://the-maazu.github.io/react-material-music-player",
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "funding": {
    "type": "liberapay",
    "url": "https://liberapay.com/the-maazu/donate"
  }
}
