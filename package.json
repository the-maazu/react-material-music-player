{
  "name": "react-material-music-player",
  "version": "2.0.0",
  "description": "This is a highly customisable music player powered by mui, typescript, mobile mode, media session action & metadata, reorderable playlist",
  "main": "dist/index.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@dnd-kit/helpers": "^0.1.19",
    "@dnd-kit/react": "^0.1.19",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.0",
    "@mui/material": "^7.0.0",
    "@mui/system": "^7.0.0",
    "@mui/utils": "^7.0.0",
    "@reduxjs/toolkit": "^2.2.1",
    "luxon": "^3.6.1",
    "react-redux": "^7.2.4"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^ 19.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/luxon": "^3.6.2",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-react-css-modules": "^0.0.0",
    "gh-pages": "^5.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "2.4.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-scripts": "^5.0.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "compile": "tsc --project tsconfig.build.json",
    "check": "tsc --noEmit",
    "prestart": "pnpm run check --watch > tsc.log &",
    "prebuild": "pnpm run check",
    "pretest": "pnpm run check",
    "start": "react-scripts --openssl-legacy-provider start",
    "build": "react-scripts --openssl-legacy-provider build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepublishOnly": "pnpm compile && pnpm emit",
    "predeploy": "pnpm run build",
    "deploy": "gh-pages -d build",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/the-maazu/react-material-music-player.git"
  },
  "keywords": [
    "react",
    "music",
    "player",
    "material",
    "ui",
    "audio",
    "typescript"
  ],
  "author": "maazudev@proton.me",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/the-maazu/react-music-player/issues"
  },
  "homepage": "https://the-maazu.github.io/react-material-music-player",
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "funding": {
    "type": "liberapay",
    "url": "https://liberapay.com/the-maazu/donate"
  },
  "packageManager": "pnpm@10.8.0"
}
